<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="@dimen/asset_end_colour">
        <shape android:shape="rectangle">
            <solid android:color="@color/default_back"/>
        </shape>
    </item>

    <item android:left="@dimen/asset_start_colour">
        <shape android:shape="rectangle">
            <solid android:color="@color/ManualScanned"/>
            <corners
                android:bottomRightRadius="@dimen/cornerround"
                android:topRightRadius="@dimen/cornerround"
                />
        </shape>
    </item>

    <item android:top="0dp" android:bottom="0dp" android:left="-2dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/summ_border" android:color="@color/roomcolour" />
            <corners
                android:bottomRightRadius="@dimen/cornerround"
                android:topRightRadius="@dimen/cornerround"
                />
            <solid android:color="#00000000" />
        </shape>
    </item>

</layer-list>